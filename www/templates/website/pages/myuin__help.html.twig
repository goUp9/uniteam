{% extends "base.html.twig" %}
{% block content %} 
    {% if data.newsletterPromtFlag %}
        {% include 'commons/newsletterPromt.html.twig'%}
    {% endif %}
    <div id="wrapper">
        {% include 'commons/header.html.twig'%}
    <main>     
        <h1>My uin help</h1>        
        {% include 'commons/myuin-nav.html.twig'%}
        <div id="videos">
            {% for video in data.videos %}
                <div class="video">
                    <iframe width="420" height="315" src="{{video.video}}" frameborder="0" allowfullscreen></iframe>
                    <p>{{video.title}}</p>
                </div>
            {%endfor%}
        </div>
    </main>
    <div id="push-footer"></div>    
    </div>
{% endblock %}